<div class="btn-group">
  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
    Select map
    <span class="caret"></span>
  </button>
  <ul class="dropdown-menu" role="menu">
  {{#each model as |mymap|}}
    {{#link-to "mapview.map" mymap.id tagName="li"}}
      <a href>{{mymap.name}}</a>
    {{/link-to}}
  {{/each}}
  </ul>
</div>

{{outlet}}
